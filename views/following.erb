<head>
    <title>Nanotwitter | Following</title>
    <link rel="stylesheet" type="text/css" href="/styles/user.css">
</head>

<div class="content">
        <% flash.keys.each do |type|%>
            <div class="alert alert-<%=type%>">
                <%= flash[type]%>
            </div>
        <% end%>
    <h1>  <%= session[:user]["username"]%>  Following </h1>
    <br>
    <br>
       <% @followings_list.each do |fan_following| %>
            <p>This is a following</p>
            <p><%= fan_following["star_displayname"] %>
        <%end%>
</div>

